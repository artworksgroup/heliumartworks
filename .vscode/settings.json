{
  "editor.codeActionsOnSave": {
    // "source.fixAll": true,
    "source.fixAll.eslint": true,
    "source.fixAll.prettier": true,
    "source.organizeImports.eslint": true,
    "source.organizeImports.prettier": true,
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "cSpell.words": [
    "stylelint",
    "tailwindcss"
  ],
}